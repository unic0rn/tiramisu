<?xml version="1.0" encoding="utf-8"?>
<commands>
	<cmd name="kick" code="KICK $active $nick :$reason"/>
	<cmd name="ban" code="MODE $active +b $host"/>
	<cmd name="unban" code="MODE $active -b $host"/>
	<cmd name="kickban" code="MODE $active +b $host\nKICK $active $nick :$reason"/>
	<cmd name="kb" code="MODE $active +b $host\nKICK $active $nick :$reason"/>
	<cmd name="kill" code="KILL $nick :$reason"/>
	<cmd name="sop" code="MODE $active +ao $nick $nick"/>
	<cmd name="desop" code="MODE $active -ao $nick $nick"/>
	<cmd name="op" code="MODE $active +o $nick"/>
	<cmd name="deop" code="MODE $active -o $nick"/>
	<cmd name="hop" code="MODE $active +h $nick"/>
	<cmd name="dehop" code="MODE $active -h $nick"/>
	<cmd name="voice" code="MODE $active +v $nick"/>
	<cmd name="devoice" code="MODE $active -v $nick"/>
</commands>
